<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:vm="using:ArchipelagoDebugClient.ViewModels.DesignData"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="ArchipelagoDebugClient.Views.SlotDataView">
    <Design.DataContext>
        <vm:SlotDataDesignData/>
    </Design.DataContext>
    <TreeDataGrid CanUserResizeColumns="True" CanUserSortColumns="False" Source="{Binding HierarchySource}"/>
</UserControl>
